# AdScaler Console Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to git - it contains sensitive credentials

# =============================================================================
# Supabase Configuration
# =============================================================================
# Get these from: https://supabase.com/dashboard/project/_/settings/api
VITE_SUPABASE_PROJECT_ID="your-supabase-project-id"
VITE_SUPABASE_PUBLISHABLE_KEY="your-supabase-anon-key"
VITE_SUPABASE_URL="https://your-project.supabase.co"

# =============================================================================
# fal.ai Configuration
# =============================================================================
# Get your key from: https://fal.ai/dashboard/keys
# Used for AI image generation (Flux model)
VITE_FAL_KEY="your-fal-api-key"

# =============================================================================
# Bannerbear Configuration
# =============================================================================
# Get your key from: https://www.bannerbear.com/app/settings/api
# Used for compositing text overlays onto AI-generated images
VITE_BANNERBEAR_API_KEY="your-bannerbear-api-key"

# =============================================================================
# n8n Webhook Configuration (Optional)
# =============================================================================
# Default: https://williamsforeal.app.n8n.cloud/webhook
# Override if using a different n8n instance
# VITE_N8N_WEBHOOK_BASE_URL="https://your-n8n-instance.com/webhook"

# =============================================================================
# Airtable Configuration
# =============================================================================
# Required for running scripts/images_schema_update_FIXED.js
# Get your Personal Access Token from: https://airtable.com/create/tokens
# Base ID can be found in your Airtable base URL: https://airtable.com/{BASE_ID}/...
# Table ID can be found in the Airtable API docs for your base
AIRTABLE_BASE_ID="your-airtable-base-id"
AIRTABLE_TABLE_ID="your-airtable-table-id"
AIRTABLE_PAT="your-airtable-personal-access-token"
